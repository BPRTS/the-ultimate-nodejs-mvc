{
  "name": "my-nodejs-app",
  "version": "1.0.0",
  "description": "A Node.js application using TypeScript, EJS, and SCSS.",
  "main": "src/app.ts",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon",
    "debug": "nodemon --inspect src/app.ts",
    "build": "tsc",
    "watch": "tsc --watch",
    "compile-scss": "sass scss/:public/css/ --style compressed --update"
  },
  "dependencies": {
    "express": "^4.17.1",
    "ejs": "^3.1.6",
    "sass": "^1.69.5"
  },
  "devDependencies": {
    "typescript": "^4.4.4",
    "nodemon": "^2.0.12",
    "ts-node": "^10.4.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "MIT"
}